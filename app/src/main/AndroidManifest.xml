<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.almatsurat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlMatsurat">
        <activity android:name=".ManfaatAct"
            android:label="Manfaat Dzikir Pagi &amp; Petang"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Tentang"
            android:label="Tentang"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PetangAct"
            android:label="Dzikir Petang"
            android:parentActivityName=".Dzikir" />
        <activity
            android:name=".PagiAct"
            android:label="Dzikir Pagi"
            android:parentActivityName=".Dzikir" />
        <activity
            android:name=".Dzikir"
            android:label="Dzikir"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>